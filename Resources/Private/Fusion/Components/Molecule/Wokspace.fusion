prototype(PunktDe.EditConflictPrevention:Components.Molecule.Workspace) < prototype(Neos.Fusion:Case) {
    userWorkspace {
        condition = ${item.userWorkspace}
        renderer = PunktDe.EditConflictPrevention:Components.Atom.WorkspaceOwner
    }

    otherWorkspace {
        condition = ${true}
        renderer = afx`
            {item.workspaceName} <br/>
            Owned by <PunktDe.EditConflictPrevention:Components.Atom.WorkspaceOwner />
        `
    }
}
